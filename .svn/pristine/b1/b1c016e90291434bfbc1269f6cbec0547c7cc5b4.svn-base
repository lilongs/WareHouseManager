using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Model
{
    public class MoIRoleInfo
    {
        #region Model
        private int _roleid;
        private string _rolename;
        private string _remark;
        /// <summary>
        /// 权限ID
        /// </summary>
        public int RoleID
        {
            set { _roleid = value; }
            get { return _roleid; }
        }
        /// <summary>
        /// 权限名称
        /// </summary>
        public string RoleName
        {
            set { _rolename = value; }
            get { return _rolename; }
        }
        /// <summary>
        /// 备注
        /// </summary>
        public string Remark
        {
            set { _remark = value; }
            get { return _remark; }
        }
        #endregion Model
    }
}
